{
  "name": "fcc-voting-app",
  "version": "1.0.0",
  "description": "A voting app for freeCodeCamp back-end certificate",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": "8.1.4"
  },
  "scripts": {
    "watch:sass": "node-sass public/src/stylesheets/sass/main.scss public/dist/stylesheets/style.css -w",
    "devserver": "nodemon -r dotenv/config app.js debug",
    "start:dev": "npm-run-all --parallel devserver watch:sass",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crhain/fcc-voting-app.git"
  },
  "author": "Carl Hain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crhain/fcc-voting-app/issues"
  },
  "homepage": "https://github.com/crhain/fcc-voting-app#readme",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "hbs": "^4.0.1",
    "method-override": "^2.3.10",
    "mongodb": "^2.2.27",
    "mongoose": "^5.0.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "passport-twitter": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "cssnano": "^3.10.0",
    "dotenv": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-minify": "^1.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2"
  }
}

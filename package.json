{
  "name": "fcc-voting-app",
  "version": "1.0.1",
  "description": "A voting app for freeCodeCamp back-end certificate",
  "main": "server.js",
  "homepage": "https://github.com/crhwebdev/fcc-voting-app",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "watch:sass": "node-sass public/src/stylesheets/sass/main.scss public/dist/stylesheets/style.css -w",
    "devserver": "nodemon -r dotenv/config app.js debug",
    "start:dev": "npm-run-all --parallel devserver watch:sass",
    "start": "node app.js",
    "test": "mocha tests/**.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crhwebdev/fcc-voting-app.git"
  },
  "author": "Carl Hain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crhwebdev/fcc-voting-app/issues"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "hbs": "^4.0.1",
    "hoek": "^5.0.3",
    "method-override": "^2.3.10",
    "mongodb": "^2.2.27",
    "mongoose": "^5.0.3",
    "mongoose-findorcreate": "^2.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "passport-twitter": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "cssnano": "^3.10.0",
    "dotenv": "^5.0.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.2"
  }
}
